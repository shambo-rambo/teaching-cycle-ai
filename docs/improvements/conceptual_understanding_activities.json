        "key_principle": "Concepts become clear through examination of attributes, examples, and connections"
      },
      "activity_structure": {
        "name": "Interactive Concept Mapping",
        "description": "Students collaboratively build visual representations of concept relationships",
        "implementation_phases": [
          {
            "phase": "concept_introduction",
            "description": "Introduce target concept with minimal definition",
            "duration_minutes": "{base_duration * 0.15}",
            "teacher_role": "concept_introducer",
            "student_role": "initial_explorer"
          },
          {
            "phase": "attribute_identification",
            "description": "Students identify key characteristics and attributes",
            "duration_minutes": "{base_duration * 0.25}",
            "teacher_role": "attribute_facilitator",
            "student_role": "characteristic_identifier"
          },
          {
            "phase": "example_generation",
            "description": "Generate examples and non-examples from multiple contexts",
            "duration_minutes": "{base_duration * 0.30}",
            "teacher_role": "example_guide",
            "student_role": "example_generator"
          },
          {
            "phase": "relationship_mapping",
            "description": "Map connections to other concepts and ideas",
            "duration_minutes": "{base_duration * 0.20}",
            "teacher_role": "connection_facilitator",
            "student_role": "relationship_mapper"
          },
          {
            "phase": "synthesis_refinement",
            "description": "Refine understanding and create final concept map",
            "duration_minutes": "{base_duration * 0.10}",
            "teacher_role": "synthesis_supporter",
            "student_role": "understanding_synthesizer"
